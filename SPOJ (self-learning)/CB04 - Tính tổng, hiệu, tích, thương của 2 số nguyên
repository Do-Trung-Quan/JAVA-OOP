import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int a = in.nextInt();
        int b = in.nextInt();
        System.out.println(a+b);
        System.out.println(a-b);
        System.out.println(a*b);
        if(b != 0){
            System.out.println(String.format("%.2f", (double)a/b));
        }else{
            System.out.println("ERROR");
        }
    }
}
