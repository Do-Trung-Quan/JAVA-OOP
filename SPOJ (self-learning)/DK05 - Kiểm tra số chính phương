import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        long a = in.nextLong();
        
        if (a < 0) {
            System.out.println("NO");
        } else {
            long sqrt = (long) Math.sqrt(a);
            if (sqrt * sqrt == a) {
                System.out.println("YES");
            } else {
                System.out.println("NO");
            }
        }
    }
}
