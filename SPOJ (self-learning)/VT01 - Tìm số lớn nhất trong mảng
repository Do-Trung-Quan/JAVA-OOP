import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        Integer[] a = new Integer[n];
        for(int i=0;i<n;i++){
            a[i] = in.nextInt();
        }
        int max = Collections.max(Arrays.asList(a)); //chỉ dùng được khi sử dụng Interger[] (not int[])
        System.out.println(max);
    }
}
