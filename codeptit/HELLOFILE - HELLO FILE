import java.io.*;

public class Solution {
    public static void main(String[] args) {
        try {
            FileReader fr = new FileReader("Hello.txt");
            BufferedReader br = new BufferedReader(fr);
            String s;
            while((s = br.readLine()) != null){
                System.out.println(s);
            }
        } catch (Exception e) {
            return;
        }
        
    }
}
    
