import java.util.*;
import java.io.*;
import java.math.*;

public class Solution {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int t = in.nextInt();
        while(t-- > 0){
            long n = in.nextLong();
            long N = (long)Math.sqrt(n);
            if(n == N*N) {
                System.out.println("YES");
            }else System.out.println("NO");
        }
    }
}

