import java.math.BigInteger; 
import java.util.*; 

public class Solution { 
    static class Khoa{
        String name;
        List<SinhVien> arr = new ArrayList<>();
        public Khoa(String a){
            this.name = a;
        }
        
        public void addSV(SinhVien a){
            this.arr.add(a);
        }
        
        @Override
        public String toString(){
            String res =  "DANH SACH SINH VIEN KHOA "+this.name+":\n";
            for(SinhVien x:this.arr){
                res += x+"\n";
            }
            return res.trim();
        }
    }

    static class SinhVien{
        String id, name, lop, mail;
        public SinhVien(String id, String name, String lop, String mail){
            this.id = id;
            this.name = name;
            this.lop = lop;
            this.mail = mail;
        }
        
        @Override
        public String toString(){
            return id+" "+name+" "+lop+" "+mail;
        }
    }
    
    public static void main(String[] args) { 
        Scanner in= new Scanner(System.in); 
        int t = in.nextInt();
        in.nextLine();
        List<SinhVien> A = new ArrayList<>();        
        while(t-- > 0){
            SinhVien sv = new SinhVien(in.nextLine(), in.nextLine(), in.nextLine(), in.nextLine());
            A.add(sv);
        }
        
        int T =in.nextInt();
        in.nextLine();
        while(T-- > 0){
            String c = in.nextLine();
            Khoa k = new Khoa(c);
            for(SinhVien x:A){
                if(x.lop.substring(1, 3).equals(c.substring(2))){
                    k.addSV(x);
                }
            }
            System.out.println(k);
        }
    }
}

