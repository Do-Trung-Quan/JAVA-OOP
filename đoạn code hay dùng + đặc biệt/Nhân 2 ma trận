// đoạn code thực hiện nhân 2 ma trận a(n,m) và A(m,n) và in ra kết quả

            for(int k=0;k<n;k++){
                for(int i=0;i<n;i++){
                    int num = 0;
                    for(int j=0;j<m;j++){
                        num+=a[k][j]*A[j][i];
                    }
                    System.out.print(num+" ");
                }
                System.out.println();
            }
